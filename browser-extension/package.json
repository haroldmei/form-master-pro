{
  "name": "formmaster-extension",
  "version": "0.1.13",
  "description": "FormMasterPro - Browser extension for form automation",
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "lint": "eslint .",
    "zip": "node scripts/zip-extension.js",
    "version:patch": "cd .. && npm run version:patch",
    "version:minor": "cd .. && npm run version:minor",
    "version:major": "cd .. && npm run version:major",
    "version:set": "cd .. && npm run version:set"
  },
  "keywords": [
    "browser-extension",
    "form",
    "automation",
    "formmaster"
  ],
  "author": "FormMasterPro Team",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.242",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.43.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
