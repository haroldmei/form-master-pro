{
  "formName": "Registration Form",
  "version": "1.0",
  "fields": [
    {
      "id": "fullName",
      "label": "Full Name",
      "type": "text",
      "required": true
    },
    {
      "id": "email",
      "label": "Email Address",
      "type": "email",
      "required": true
    },
    {
      "id": "age",
      "label": "Age",
      "type": "number",
      "required": false
    },
    {
      "id": "interests",
      "label": "Interests",
      "type": "checkbox",
      "options": ["Sports", "Music", "Art", "Technology", "Travel"],
      "required": false
    }
  ],
  "submitButton": {
    "text": "Submit Form",
    "bgColor": "#28a745"
  }
}
